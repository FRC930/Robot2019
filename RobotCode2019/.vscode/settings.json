{
    "java.configuration.updateBuildConfiguration": "automatic",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "bin/": true,
        ".classpath": false,
        ".project": false
    },
    "git.ignoreLimitWarning": true,
    "java.test.config": {
        "name": "withLibs",
        "env": {
            "path": "${workspaceFolder}\\build\\tmp\\jniExtractDir"
        }
    },
    "java.test.defaultConfig": "withLibs"
}